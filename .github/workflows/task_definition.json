{
    "family": "llmopstdlive",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::548481212060:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "8192",
    "containerDefinitions": [
      {
        "name": "llmops-live-container",
        "image": "548481212060.dkr.ecr.ap-south-2.amazonaws.com/llmopsrepolive",
        "cpu": 1024,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "name": "llmops-live-container-8080-tcp",
            "appProtocol": "http"
          }
        ],
        "environment": [
          {
            "name": "ENV",
            "value": "production"
          }
        ],
        "secrets": [
          {
            "name": "key1",
            "valueFrom": "arn:aws:secretsmanager:ap-south-2:548481212060:secret:key1-QcBdAq"
          }
  
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/llmopstdlive",
            "awslogs-region": "ap-south-2",
            "awslogs-stream-prefix": "ecs",
            "awslogs-create-group": "true"
          }
        }
      }
    ]
  }
  